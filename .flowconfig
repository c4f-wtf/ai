[ignore]

[include]

[libs]

[lints]

[options]
esproposal.decorators=ignore
esproposal.optional_chaining=ignore
module.ignore_non_literal_requires=true

module.name_mapper='^src\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^src$' -> '<PROJECT_ROOT>/src'
module.name_mapper='^@store\/\(.*\)$' -> '<PROJECT_ROOT>/src/store/\1'
module.name_mapper='^@store$' -> '<PROJECT_ROOT>/src/store'
module.name_mapper='^@page\/\(.*\)$' -> '<PROJECT_ROOT>/src/component/page/\1'
module.name_mapper='^@page$' -> '<PROJECT_ROOT>/src/component/page'
module.name_mapper='^@iframe\/\(.*\)$' -> '<PROJECT_ROOT>/src/iframe/\1'
module.name_mapper='^@iframe$' -> '<PROJECT_ROOT>/src/iframe'
module.name_mapper='^@component\/\(.*\)$' -> '<PROJECT_ROOT>/src/component/\1'
module.name_mapper='^@component$' -> '<PROJECT_ROOT>/src/component'
module.name_mapper='^@element\/\(.*\)$' -> '<PROJECT_ROOT>/src/component/element/\1'
module.name_mapper='^@element$' -> '<PROJECT_ROOT>/src/component/element'

module.name_mapper='^@localdb$' -> '<PROJECT_ROOT>/src/localdb.js'
module.name_mapper='^@util$' -> '<PROJECT_ROOT>/src/util.js'
module.name_mapper='^@types$' -> '<PROJECT_ROOT>/src/types.js'
module.name_mapper='^@shared-styles$' -> '<PROJECT_ROOT>/src/component/shared-styles.css'

[strict]
